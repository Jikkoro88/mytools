find . -type f -print | grep -v libmysqld | grep -v bzr | grep -v storage\/ndb | grep -v \.git | grep -v cscope | grep '\.c$' > cscope.files
find . -type f -print | grep -v libmysqld | grep -v bzr | grep -v storage\/ndb | grep -v \.git | grep -v cscope | grep '\.h$' >> cscope.files
find . -type f -print | grep -v libmysqld | grep -v bzr | grep -v storage\/ndb | grep -v \.git | grep -v cscope | grep '\.ic$' >> cscope.files
find . -type f -print | grep -v libmysqld | grep -v bzr | grep -v storage\/ndb | grep -v \.git | grep -v cscope | grep '\.cc$' >> cscope.files
echo Start build for $( wc -l cscope.files ) 
cscope -b -q -k

